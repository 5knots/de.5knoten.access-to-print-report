<?xml version="1.0"?>
<extension key="de.5knoten.access-to-print-report" type="module">
  <file>access_to_print_report</file>
  <name>Access to "Print Report"</name>
  <description>Manages access to the "Print Report" button in CiviCase. Gives access to all users with the right "access my cases" instead of only users with the right "access all cases".</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Katharina Ullherr</author>
    <email>katharina.ullherr@5knoten.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://access-to-print-report</url>
    <url desc="Documentation">http://access-to-print-report</url>
    <url desc="Support">http://access-to-print-report</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-11-26</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Funded by Antidiskriminierungsverband Deutschland, developed with support of the CiviCRM community</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/AccessToPrintReport</namespace>
  </civix>
</extension>
